{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "edge": "17",
          "firefox": "50",
          "chrome": "67",
          "safari": "11.1",
          "ie": "9"
        },
        "useBuiltIns": "entry",
        "corejs": "2"
      }
      
    ]
    
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}
